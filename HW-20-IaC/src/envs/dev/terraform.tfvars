vpc_cidr_block      = "10.0.0.0/16"  # CIDR блок для VPC, що визначає діапазон IP-адрес для приватної мережі
public_subnet_cidr  = "10.0.1.0/24"  # CIDR блок для публічної підмережі, де будуть розміщені ресурси (інстанси)
instance_type       = "t3.micro"     # Тип EC2 інстансу, використовується "t3.micro" для низьковитратного інстансу
list_of_open_ports  = [80, 443]      # Список портів для відкриття в Security Group (80 для HTTP, 443 для HTTPS)
